package mundo;

import java.sql.Date;

public class prueba {
	
	
public static void main(String[] args) {

	
	
Producto pro = new 	Producto("camisa",(short)3,12300f,"nacional",10f);	
	
	
Pedido ob = new Pedido(pro,Estado.PROCESANDO, new Date(12/02/2020));
	
Pago pa = new Pago(ob);


Tarjetadecredito tar = new Tarjetadecredito("1234567",Entidad.VISA, new Date(12/02/2022),1345678f);


pa.pagarPedido(tar, 0);





}
	

}
